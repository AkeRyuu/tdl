{
  "name": "tdl",
  "version": "3.5.2",
  "description": "TDLib (Telegram Database library) bindings for Node.js",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "types": "flow-copy-source src dist",
    "build": "babel src/ -d dist/ && npm run types",
    "lint": "eslint . --max-warnings 0",
    "test": "flow check && npm run lint",
    "prepare": "npm run clean && npm run build",
    "prepack": "npm test",
    "flow:start": "pm2 start tdl-flow.sh",
    "flow:stop": "pm2 stop tdl-flow",
    "flow:restart": "npm run flow:stop && npm run flow:start",
    "flow:status": "flow status",
    "start": "node dist"
  },
  "files": [
    ".flowconfig",
    "dist",
    "vendor",
    "flow-typed",
    "flow",
    "README.md"
  ],
  "dependencies": {
    "debug": "^3.1.0",
    "eventemitter3": "^3.0.1",
    "ffi-napi": "^2.4.3",
    "fluture": "^8.0.2",
    "promptly": "^3.0.3",
    "ramda": "^0.25.0",
    "ref-napi": "^1.4.0"
  },
  "devDependencies": {
    "@bannerets/eslint-config-flow": "^0.2.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.19.1",
    "flow-bin": "^0.69.0",
    "flow-copy-source": "^1.3.0",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "author": "Bannerets <save14@protonmail.com>",
  "license": "MIT",
  "keywords": [
    "telegram",
    "tdlib",
    "mtproto",
    "ffi",
    "tglib"
  ],
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bannerets/tdl.git"
  },
  "bugs": {
    "url": "https://github.com/Bannerets/tdl/issues"
  },
  "homepage": "https://github.com/Bannerets/tdl#readme"
}
